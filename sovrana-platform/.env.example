# NextAuth Configuration
NEXTAUTH_SECRET=your-secret-key-here
NEXTAUTH_URL=http://localhost:3000
AUTH_EMAIL=your-email@example.com
AUTH_PASSWORD=your-password-here

# Polymarket API Configuration
# Copy this file to .env.local and fill in your credentials

# Wallet Address
POLY_ADDRESS=0x_YOUR_WALLET_ADDRESS

# CLOB API Credentials (L2 Authentication)
POLY_API_KEY=your-api-key-here
POLY_SECRET=your-base64-secret-here
POLY_PASSPHRASE=your-passphrase-here

# API Base URLs
GAMMA_API_URL=https://gamma-api.polymarket.com
DATA_API_URL=https://data-api.polymarket.com
CLOB_API_URL=https://clob.polymarket.com

# Private Key (L1 Authentication - Order Signing)
PRIVATE_KEY=0x_YOUR_PRIVATE_KEY

# Chain Configuration
CHAIN_ID=137
